<%- include("header") %> 

    <div class="box" id="heading">
        <h1><%= Today %></h1>
    </div>

    <div class="box">
        <%for(var i=0;i<Items.length;i++){%>
            <div class="item">
                <input type="checkbox" name="" id="">
                <p><%= Items[i] %></p>
            </div>

        <%}%>
        <form class="item" action=<%= Path %> method="post">
            <input type="text" name="newItem" placeholder="New Item" class="item">
            <button type="submit" name="button">+</button>
        </form>
    </div>
<%- include("footer") %> 
